 * WordEntry class
 *
 * This class acts as an object that can hold a word and the number of occurences of that word.
 *
 */

#include "WordEntry.h"

/**
 * WordEntry::WordEntry()
 * Constructor to create an empty WordEntry object.
 */
WordEntry::WordEntry()
{
  wordKey = "";
  wordCount = 0;
}

/**
 * WordEntry::WordEntry(string, int)
 * Constructor to create a WordEntry object with a word and its count.
 */
WordEntry::WordEntry(string word, int count)
{
  wordKey = word;
  wordCount = count;
}
